{
  "manifest_version": 3,
  "name": "Visual Bookmark Tree",
  "version": "1.0",
  "description": "显示Chrome书签的树状结构",
  "permissions": ["bookmarks"],
  "host_permissions": ["https://www.google.com/"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
} 